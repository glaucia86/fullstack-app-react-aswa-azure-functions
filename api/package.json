{
  "name": "api",
  "version": "1.0.0",
  "description": "A simple API with Azure Functions, Prisma and Azure Cosmos DB for MongoDB API",
  "main": "dist/src/functions/*.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rimraf dist",
    "dev": "concurrently \"npm run watch\" \"npm run start\"",
    "start:dev": "concurrently \"npm run watch\" \"func start\"",
    "prestart": "npm run clean && npm run build",
    "start": "func start",
    "test": "echo \"No tests yet...\""
  },
  "dependencies": {
    "@azure/cosmos": "^4.2.0",
    "@azure/functions": "^4.0.0",
    "@prisma/client": "^6.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/node": "^18.19.74",
    "azure-functions-core-tools": "^4.x",
    "concurrently": "^9.1.2",
    "prisma": "^6.2.1",
    "rimraf": "^5.0.0",
    "typescript": "^4.9.5"
  }
}
